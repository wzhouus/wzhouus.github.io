---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Wei's Blog</title>
	<link href="http://wzhouus.github.io/blog/atom.xml" rel="self"/>
	<link href="http://wzhouus.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://wzhouus.github.io/blog</id>
	<author>
		<name>Wei</name>
		<email>wzhouus@gmail.com</email>
	</author>

	{% for post in site.posts %}
	    {% if post.categories contains 'ne2' %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://wzhouus.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://wzhouus.github.io/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	    {% endfor %}
	{% endfor %}
</feed>
